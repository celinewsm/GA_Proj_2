<h1>Information on stall</h1>


<form action="/" method="POST" role="form">
  <div class="form-group">
    <label for="name">Name</label>
    <input type="textarea" class="form-control" id="name" placeholder="e.g., Ah Seng Fishball Noodles" name="name">
  </div>
  <div class="form-group">
    <label for="name">Address</label>
    <input type="textarea" class="form-control" id="address" placeholder="e.g., 32 Gamba Avenue" name="address">
  </div>
  <div class="form-group">
    <label for="name">Account Manager</label>
    <input type="textarea" class="form-control" id="accountMgr" placeholder="e.g., John" name="accountMgr">
  </div>
  <div class="form-group">
    <label for="name">Last Order</label>
    <input type="textarea" class="form-control" id="lastOrders" placeholder="e.g., 5 kg whitefish balls" name="lastOrders">
  </div>
  <div class="form-group">
    <label for="name">Contact Number</label>
    <input type="textarea" class="form-control" id="contactNo" placeholder="e.g., Jane 91112222" name="contactNo">
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<a href="/">&larr; Back</a>




